{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "chrome >= 49",
            "firefox >= 45",
            "edge >= 14",
            "ie >= 11",
            "safari >= 10"
          ]
        }
      }
    ],
    "react"
  ],
  "plugins": [
    "babel-plugin-syntax-dynamic-import",
    "transform-object-rest-spread"
  ]
}
